{
  "name": "a11y-outline",
  "version": "1.1.0",
  "description": "a11y-outline - web extension to navigate page outlines easily",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thisiskylierose/a11y-outline.git"
  },
  "bugs": {
    "url": "https://github.com/thisiskylierose/a11y-outline/issues"
  },
  "homepage": "https://github.com/thisiskylierose/a11y-outline#readme",
  "main": "index.js",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "rimraf ./dist && tsc",
    "watch:bundle": "npm-watch bundle",
    "watch:template": "npm-watch template",
    "rimraf": "rimraf ./dist",
    "bundle": "node scripts/bundle",
    "template": "node scripts/template",
    "start": "npm-run-all -p watch:bundle watch:template",
    "lint": "eslint --ext .ts src/ --fix",
    "dist": "npm-run-all build bundle template",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "watch": {
    "bundle": {
      "patterns": [
        "./src"
      ],
      "extensions": "ts",
      "quiet": false
    },
    "template": {
      "patterns": [
        "./dist"
      ],
      "extensions": "js",
      "quiet": false
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run check"
    }
  },
  "dependencies": {
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "aria-api": "0.4.0",
    "browserify": "17.0.0",
    "cross-env": "7.0.3",
    "dialog-polyfill": "0.5.6",
    "eslint": "7.22.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.2.0",
    "npm-run-all": "4.1.5",
    "npm-watch": "0.9.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sass-loader": "11.0.1",
    "tinyify": "3.0.0",
    "tsify": "5.0.2",
    "typescript": "4.2.3"
  }
}
